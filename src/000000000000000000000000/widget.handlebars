      </div>

      <!-- online and local tab headers -->
      <label class="ps-online-tab-label" for="onlineTab">
          Find Online
      </label>
      <label class="ps-local-tab-label" for="localTab">
          Find Nearby
      </label>



    </div>

    <!-- online section -->
    <div class="ps-online-container ps-online-tab ps-float-box">
     <div class="ps-online-section ps-float-box">
         <div class="ps-online-sellers ps-float-box" data-item="onlineSellers">
             <div class="ps-online-seller-listings ps-float-box">
             {{#if onlineSellers}}
                 {{#each onlineSellers}}
                     <div class="ps-online-seller-select ps-online-seller-listing ps-float-box" data-seller="{{seller.id}}">
                         <div class="ps-online-seller-listing-cell ps-online-seller-cell ps-float-box">
                             {{#if seller.hasImage}}
                                 <img src="{{seller.imageUrl}}" title="{{seller.name}}">
                             {{else}}
                                 <div class="ps-online-seller-listing-name">
                                     {{seller.name}}
                                 </div>
                             {{/if}}
                             <div class="ps-seller-error-name">
                                 {{seller.name}}
                             </div>
                             <div class="ps-seller-error-name">
                                 {{seller.name}}
                             </div>
                         </div>
                         <div class="ps-online-seller-listing-cell ps-online-price-cell ps-float-box">
                             {{#if price}}
                                 {{#if hiddenPrice}}
                                     <div>{{@root.res/hiddenPriceLabel}}</div>
                                 {{else}}
                                     <div>{{formatCurrency price}}</div>
                                 {{/if}}
                             {{else}}
                                 <div>--</div>
                             {{/if}}
                             {{#if-equal shippingCost 0}}
                                 <div>
                                     <span>&#43;</span>
                                     <span>{{{@root.res.freeShipping}}}</span>
                                 </div>
                             {{else}}
                                 {{#if shippingCost}}
                                     <div>
                                         <span>&#43;</span>
                                         <span>{{formatCurrency shippingCost}} {{{@root.res.shipping}}}</span>
                                     </div>
                                 {{/if}}
                             {{/if-equal}}
                         </div>
                         <div class="ps-online-seller-listing-cell ps-online-availability-cell ps-float-box">
                             {{#if status.stock}}
                                 {{#if status.inStock}}
                                     <div class="ps-in-stock">
                                         <svg xmlns="http://www.w3.org/2000/svg" width="10px" height="10px" viewBox="0 0 10 10">
                                             <path fillRule="evenodd" d="M 0.01 6.17C 0.01 6.17 4.44 9.99 4.44 9.99 4.44 9.99 10.01 1.19 10.01 1.19 10.01 1.19 8.32 0 8.32 0 8.32 0 3.98 6.41 3.98 6.41 3.98 6.41 1.23 4.63 1.23 4.63 1.23 4.63 0.01 6.17 0.01 6.17Z" />
                                         </svg>
                                         {{status.stock}}
                                     </div>
                                 {{else}}
                                     <span>{{status.stock}}</span>
                                 {{/if}}
                             {{else}}
                                 <span>{{{@root.res.seeWebsite}}}</span>
                             {{/if}}
                         </div>
                         <div class="ps-online-seller-listing-cell ps-online-buy-cell ps-float-box">
                             {{#if status.inStock}}
                                 <button class="ps-online-buy-button">Buy</button>
                             {{else}}
                                 <button class="ps-online-buy-button">{{{@root.res.view}}}</button>
                             {{/if}}
                         </div>
                     </div>
                 {{/each}}
             {{else}}
                 <div class="ps-no-results">
                     <p><b>{{{@root.res.sorry}}}</b><br />{{{@root.res.sorryOnline}}}</p>
                 </div>
             {{/if}}
         </div>
         </div>
     </div>
 </div>

 <!-- *************** local section *************** -->
 <div class="ps-local-container ps-local-tab ps-float-box">
   <!-- map -->
   <div class="ps-map-holder">
      <div class="ps-map"  data-offset="14.5"></div>
    </div>

    <!-- local sellers -->

    {{#if localSellers}}
      <div class="ps-local-sellers ps-float-box">
          {{#each localSellers}}
              <div class="ps-map-pushpin-select ps-float-box" data-store="{{store.id}}" data-action="popup">
                  <div class="ps-local-left-mobile">
